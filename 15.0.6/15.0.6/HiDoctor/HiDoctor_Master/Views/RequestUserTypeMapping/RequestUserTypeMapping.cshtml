@{
    ViewBag.Title = "RequestUserTypeMapping";
    Layout = null;
}
<div>
    <div class="col-lg-12">
        <div class="col-lg-2">
            <label>Request</label>
        </div>
        <div class="col-lg-2">
            <select id="drpRequestName" class="input-medium form-control"></select>
        </div>
    </div>
    <div style="clear: both"></div>
    <div class="col-lg-12" style="margin-top: 10px">
        <div class="col-lg-2">
            <label>User Type</label>
        </div>
        <div class="col-lg-2">
            <select id="drpUserTypeName" class="input-medium form-control"></select>
        </div>
    </div>
    <div style="clear: both"></div>

    <div class="col-lg-12 form-group">
        <button style="margin-left: 1%" id="btnSave" type="button" class="btn btn-primary" onclick="fnSave()">Save</button>
        <button id="btnUpdate" style="display: none" type="button" class="btn btn-primary" onclick="fnUpdate()">Update</button>
        <button id="btnCancel" type="button" class="btn btn-primary" onclick="fnCancel()">Cancel</button>
        <input type="hidden" id="hdRequestCode" />
    </div>
    <lable id="lblmessage" style="font-size: 15px; margin-left: 37%; color: red; text-align: center"></lable>
    <div id="dvTable" class="col-lg-12"></div>
</div>
<script type="text/javascript" src="../../Areas/HiDoctor/Scripts/HD/common.js?v=@HiDoctor.BuildNumber.BuildNo"></script>
<script src="../../Areas/HiDoctor_Master/Scripts/HD_Master/RequestMapping.js?v=@HiDoctor.BuildNumber.BuildNo"></script>
<link href="~/Content/Bootstrap/bootstrap.css" rel="stylesheet" />
<script src="~/Scripts/Bootstrap/bootstrap.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        AjaxGlobalHandler.Initiate();
        try {
            $("#dvAjaxLoad").hide();
            fnUnderUserType();
            fnFillRequest();
            fnfillMappingGrid();
        }
        catch (err) {
            $("#dvAjaxLoad").hide();
        }
    });
</script>
