@{
    ViewBag.Title = "RequestCategoryMaster";
    Layout = null;
}
<style>
    .lblText {
        width: 10%;
    }
</style>
<div>
    <div class="col-lg-12">
        <div>
            <label class="lblText" style="margin-left: 3px">Request Category<font color="red">*</font></label><input type="text" style="width: 11%" id="txtRequestCategory" />
        </div>
        <div style="margin-left: 10%">
            <input type="radio" value="1" id="Enable" name="rptOptions" checked="checked" />
            <label for="EnableData">Enable</label>
            <input type="radio" value="0" id="Disable" name="rptOptions" />
            <label for="DisableData">Disable</label>
        </div>
        <div style="margin-left: 10%">
            <button id="btnSave" type="button" class="btn btn-primary" onclick="fnRequestSave()">Save</button>
            <button id="btnUpdate" style="display: none" type="button" class="btn btn-primary" onclick="fnUpdate()">Update</button>
            <button id="btnCancel" type="button" class="btn btn-primary" onclick="fnCancel()">Reset</button>
        </div>
        <div>
            <label>
                <font color="red">* Mandatory field</font>
            </label>
        </div>
        <lable id="lblmessage" style="font-size: 15px; margin-left: 37%; color: red; text-align: center"></lable>
        </>
    <div style="clear: both"></div>
        <div style="margin-left: 1%">
            @Html.ActionLink("Download Request Category Master details", "PutRequestCategoryMasterIntoExcel",new {})
        </div>
        <div style="clear: both"></div>
        <div id="dvTable" class="col-lg-12"></div>
        <div style="clear: both"></div>
    </div>



    <script type="text/javascript" src="../../Areas/HiDoctor/Scripts/HD/common.js"></script>
    <script src="../../Areas/HiDoctor_Master/Scripts/HD_Master/RequestMaster.js?v=@HiDoctor.BuildNumber.BuildNo"></script>
    <link href="~/Content/Bootstrap/bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/Bootstrap/bootstrap.js"></script>

    <script type="text/javascript">

        $(document).ready(function () {
            AjaxGlobalHandler.Initiate();
            try {
                $("#dvAjaxLoad").hide();
                fnfillRequestCategoryGrid();
                
            }
            catch (err) {
                $("#dvAjaxLoad").hide();
            }
        });
    </script>
