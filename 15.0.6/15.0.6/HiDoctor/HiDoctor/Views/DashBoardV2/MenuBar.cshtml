@{
    Layout = null;
    ViewBag.Title = "MenuBar";
    
}


<style>
    .cls-back-dashboard {
        background-color: #EEEEEE !important;
        color: white !important;
        flex: 6;
    }

    .cls-back-dash {
    }

    .cls-unapproved-main .panel-body ul {
        display: inline-block;
        list-style: none;
        width: 100%;
    }

        .cls-unapproved-main .panel-body ul li {
            float: left;
            width: 30%;
        }

            .cls-unapproved-main .panel-body ul li p.numbers {
                font-size: 25px;
                margin-bottom: 4px !important;
            }

            .cls-unapproved-main .panel-body ul li p.text {
                font-size: 13px;
                margin-bottom: 0px !important;
            }

    #main {
        margin: 0px !important;
    }
</style>

<link href="~/Content/SideMenuBar.css" rel="stylesheet" />
<link href="~/Content/DashBoard/DashBoardV2.css" rel="stylesheet" />
<link href="~/Content/Bootstrap/bootstrap.css" rel="stylesheet" />
<link href="~/Content/Bootstrap/awesome-bootstrap-checkbox.css" rel="stylesheet" />
<link href="~/Content/Bootstrap/font-awesome.min.css" rel="stylesheet" />
<div>
    <div class="col-xs-1">
        <div class="" style="left: 0px; background-color: white; padding: 2px; position: fixed; flex: 1;">
            <div id="mySidenav" class="sidenav" style="height: 100%; padding-top: 0px; top: -2px; background: none; position: relative;">
                <a href="#" title="Dashboard-1" class="cls-back-dashboard" id="DashboardV2Menu" style="padding: 15px 10px; border-bottom: 1px solid #fff; background: none;">
                    <img src="~/Images/dashboard-1.png" style="width: 75%; height: 75%; display: block;" /></a>
                <a href="#" title="Dashboard-2" class="cls-back-dash" id="DashboardV2Phase2" style="padding: 15px 10px; border-bottom: 1px solid #fff; background: none;">
                    <img src="~/Images/dashboard-2.png" style="width: 75%; height: 75%; display: block;" /></a>
                <a href="#" title="Dashboard-3" class="cls-back-dash" id="DashboardV2Phase3" style="padding: 15px 10px; border-bottom: 1px solid #fff; background: none;">
                    <img src="~/Images/dashboard-3.png" style="width: 75%; height: 75%; display: block;" /></a>
                <a href="#" title="Insights" class="cls-back-dash" id="DashboardOld" style="padding: 15px 10px; border-bottom: 1px solid #fff; background: none;">
                    <img src="~/Images/insights.png" style="width: 75%; height: 75%; display: block;" /></a>
            </div>

        </div>
    </div>
    <div class="col-xs-11" style="">
        <div class="cls-notification_top" id="divMenu_top" style="display: none;">
            <section class="dash-content_top  form-container_top">
                <span class="dash-title_top">Insights - @DateTime.Now.ToString("MMMM")  @DateTime.Now.ToString("yyyy")</span>
                <ul class="notifications_top" style="float: right !important;">
                    <li>
                        <div class="cls-dv-separator_top"></div>
                    </li>
                    <li>
                        <a href="#" class="notification-icon messages-pop" data-toggle="dropdown" title="Message">
                            @*   <i class="fa fa-envelope"></i>*@
                            <img src="~/Content/images/Home/ic_message.png" />
                            <span class="badge" id="spnMsg">0</span>
                        </a>

                    </li>
                    <li>
                        <a href="#" class="notification-icon notify-pop" data-toggle="dropdown" title="Notification">
                            @* <i class="fa fa-bell"></i>*@<img src="~/Content/images/Home/ic_Alert.png" />
                            <span class="badge" id="spnNotify">0</span>
                        </a>
                    </li>
                    <li id="ICEtask">
                        <a href="#" class="notification-icon task-pop" data-toggle="dropdown" title="Task">
                            <img src="~/Content/images/Home/ic_Task.png" style="width: 28px; height: 28px;" />
                            <span class="badge" id="spntask">0</span>
                        </a>
                    </li>
                    <li>
                        <div class="cls-dv-separator_top"></div>
                    </li>
                    <li>
                        <a href="#" class="notification-icon birthday-pop" data-toggle="dropdown" title="Birthday">
                            @*   <i class="fa fa-gift"></i>*@<img src="~/Content/images/Home/ic_birthday.png" />
                            <span class="badge" id="spnBirthday">0</span>
                        </a>
                    </li>
                    <li>
                        <a href="#" class="notification-icon anniversary-pop" data-toggle="dropdown" title="Anniversary">
                            @* <i class="fa fa-calendar-o"></i>*@
                            <img src="~/Content/images/Home/ic_anniversary.png" />
                            <span class="badge" id="spnAnniversary">0</span>
                        </a>
                    </li>
                    @*//For Admin only*@
                    <li id="liEmpBir">
                        <a href="#" class="notification-icon employee-pop" data-toggle="dropdown" title="Employee Birthday">
                            @* <i class="fa fa-calendar-o"></i>*@
                            <img style="width: 27px;" src="data:image/svg+xml;utf8;base64," />
                            <span class="badge" id="spnEmployee">0</span>
                        </a>
                    </li>
                    @*For useronly*@
                    <li id="liholiday">
                        <a href="#" class="notification-icon holiday-pop" data-toggle="dropdown" title="Holiday">
                            @* <i class="fa fa-calendar-o"></i>*@
                            <img src="~/Content/images/Home/ic_holiday.png" width="26px" height="27px" />
                            <span class="badge" id="spnHoliday">0</span>
                        </a>
                    </li>

                </ul>
            </section>
        </div>
        <div class="menuBar">


            <div class="col-xs-11" style="padding-left: 2px;">
                <div class="menuflip" id="dashboard1" style="width: 1022px;"></div>
            </div>
            <div class="" style="left: 87%; position: fixed;">
                <div class="notification" id="dashNotification">
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/HD/AdminDashboard.js"></script>
<script src="~/Scripts/HD/Dashboard.js"></script>
<script src="~/Scripts/HD/DashBoardV2.js"></script>

<script type="text/javascript">
    $(document).ready(function () {
        debugger;
        var g_user_type = "";
        $("#dvAjaxLoad").hide();
        $('#page-header').hide();
        $('#main').css('margin', "0px !important");
        AjaxGlobalHandler.Initiate();
        //$('#notifyhide').hide();
        debugger;
        var user = AdmindashboardV2.getDashboardHome();
        AdmindashboardV2.getNotificationPage();
        if (user == 'FIELD_USER') {
            //Field_user
            $("#liEmpBir").hide();
            User_Dashboard.getUserDashboardLiveCounts();
            User_Dashboard.getUserTaskLiveCounts();
        }
        else {
            //Admin
            $("#liholiday").hide();
            AdminDashboard.getAdminDashboardLiveCounts();
        }


        AdminDashboard.getDashboardHome();

        $("#dashboard1").load('DashboardV2/DashBoardV2');
        $('#DashboardV2Menu').unbind('click').bind('click', function () {
            $(this).removeClass("cls-back-dash").addClass("cls-back-dashboard");
            $("#DashboardV2Phase2").removeClass("cls-back-dashboard").addClass("cls-back-dash");
            $("#DashboardOld").removeClass("cls-back-dashboard").addClass("cls-back-dash");
            $("#DashboardV2Phase3").removeClass("cls-back-dashboard").addClass("cls-back-dash");
            $('#dashboard1').load('DashboardV2/DashBoardV2');
        });

        $('#DashboardV2Phase2').unbind('click').bind('click', function () {
            $(this).removeClass("cls-back-dash").addClass("cls-back-dashboard");
            $("#DashboardV2Menu").removeClass("cls-back-dashboard").addClass("cls-back-dash");
            $("#DashboardV2Phase3").removeClass("cls-back-dashboard").addClass("cls-back-dash");
            $("#DashboardOld").removeClass("cls-back-dashboard").addClass("cls-back-dash");
            $('#dashboard1').load('DashboardV2/Index');
        });

        $('#DashboardV2Phase3').unbind('click').bind('click', function () {
            $(this).removeClass("cls-back-dash").addClass("cls-back-dashboard");
            $("#DashboardV2Menu").removeClass("cls-back-dashboard").addClass("cls-back-dash");
            $("#DashboardV2Phase2").removeClass("cls-back-dashboard").addClass("cls-back-dash");
            $("#DashboardOld").removeClass("cls-back-dashboard").addClass("cls-back-dash");
            $('#dashboard1').load('DashboardV2/OzoneDashBoard');
        });
        $('#DashboardOld').unbind('click').bind('click', function () {

            $(this).removeClass("cls-back-dash").addClass("cls-back-dashboard");
            $("#DashboardV2Menu").removeClass("cls-back-dashboard").addClass("cls-back-dash");
            $("#DashboardV2Phase2").removeClass("cls-back-dashboard").addClass("cls-back-dash");
            $("#DashboardV2Phase3").removeClass("cls-back-dashboard").addClass("cls-back-dash");
            AdmindashboardV2.loadDashboardPage();

        });


        //$("#DashboardV2Menu").hover(function () {

        //});
        //$("#DashboardV2Phase2").hover(function () {

        //});



        //    $(this).removeClass("cls-back-dash").addClass("cls-back-dashboard");
        //    $("#DashboardV2Menu").removeClass("cls-back-dashboard").addClass("cls-back-dash");
        //    $("#DashboardV2Phase2").removeClass("cls-back-dashboard").addClass("cls-back-dash");
        //    $('#dashboard1').load('Dashboard/AdminDashboard');
        //});
    });
</script>
