@{
    ViewBag.Title = "DieticianReporting";
    Layout = "~/Views/Shared/_CCMCommonLayout.cshtml";
}
<div class="mt-3">
    <h4> Dietician Reporting</h4>
   <div id="bind">

   </div>
</div>

<div class="form-group row" style="margin-left: 71%;margin-top: 10px;" id="submit">
    <button type="button" class="btn btn-primary mb-2" style="margin-left: 4%;" onclick="DieticianReporting.fnsubmit();">Submit</button>
</div>
<link href="../../Content/token-input.css?v=@HiDoctor.BuildNo.Build_No" rel="stylesheet" type="text/css" />
<link href="../../Content/token-input-facebook.css?v=@HiDoctor.BuildNo.Build_No" rel="stylesheet" type="text/css" />
<script src="~/Scripts/jquery.tokeninput.js"></script>
<script src="../../Areas/HiDoctor_Activity/Scripts/CoreREST.js?v=@HiDoctor.BuildNo.Build_No"></script>
<script src="../../Areas/HiDoctor_Activity/Scripts/RPACoreREST.js"></script>
<script src="~/Areas/HiDoctor_Activity/Scripts/HD/DieticianReporting/DieticianReporting.js?v=@HiDoctor.BuildNo.Build_No"></script>
<script type="text/javascript">
    var CompanyCode = "";
    var RegionCode = "";
    var UserCode = "";
    var UserTypeCode="";
    var companyCode="";
    var regionCode="";
    var userCode="";
    var userTypeCode="";
    var dcrdate="";
    var UserName="";
    var RegionName="";
    var DoctorEntryMode_g="";
    $(document).ready(function () {
        debugger;
        AjaxGlobalHandler.Initiate();
        HideModalPopup('dvLoading');
        $('#dvAjaxLoad').hide();
        CompanyCode = @Html.Raw(Json.Encode(ViewBag.CompanyCode));
        RegionCode = @Html.Raw(Json.Encode(ViewBag.RegionCode));
        UserCode = @Html.Raw(Json.Encode(ViewBag.UserCode));
        UserTypeCode=@Html.Raw(Json.Encode(ViewBag.UserTypeCode));
        dcrdate=@Html.Raw(Json.Encode(ViewBag.dcrdate));
        UserName=@Html.Raw(Json.Encode(ViewBag.Username));
        RegionName=@Html.Raw(Json.Encode(ViewBag.RegionName));
        DoctorEntryMode_g = fnGetPrivilegeValue('RIGID_ATTENDANCE_DOCTOR_ENTRY', 'YES');
        DieticianReporting.defaults.companyCode=CompanyCode;
        DieticianReporting.defaults.regionCode=RegionCode;
        DieticianReporting.defaults.userCode=UserCode;
        DieticianReporting.defaults.userTypeCode=UserTypeCode;
        DieticianReporting.Init();
        $('#timepicker').timepicker({
            showPeriod: true,
            showLeadingZero: true
        });

    });
</script>
