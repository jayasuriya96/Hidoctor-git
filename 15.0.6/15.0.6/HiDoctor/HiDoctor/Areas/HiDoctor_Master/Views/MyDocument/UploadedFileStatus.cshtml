@{
    ViewBag.Title = "UploadedFileStatus";
    Layout = null;
}

<link href="~/Content/Bootstrap/bootstrap.css" rel="stylesheet" />
<script src="~/Scripts/Bootstrap/bootstrap.js"></script>
<style>
    .clsLegend {
        color: blue;
        font-weight: bold;
        font-size: 14px;
    }

    .cls_AltTitle {
        width: 100%;
        background-color: #0173BA;
        color: white;
        font-weight: bold;
        font-size: 15px;
        text-align: center;
    }

    .cls_spnTitle {
        font-weight: bold;
        font-style: italic;
        font-size: 12px;
    }
</style>

<div class="col-lg-12">
    <div id="dvLegend" style="background-color: #eee; padding: 5px;"></div>
    <div id="dvFilecount"></div>
</div>

<div id="dvLoadingInvalidFiles" style="padding-top: 5px; padding-right: 10px; display: none; width: 700px; height: 400px; border: 4px solid #CCCCCC; background-color: #FFFFFF;">
    <div style="position: absolute; font-size: 11px; width: 98%; padding: 10px;">
        <span style="float: right; cursor: pointer; color: white; margin: -23px;" onclick="HideModalPopup('dvLoadingInvalidFiles');">
            <img src="~/Images/close.png" />
        </span>
        <div style="width: auto;" class="cls_AltTitle">Invalid File Details</div>
        <div id="dvshowTitle"></div>
        <br />
        <div id="dvShowinvalidFiles" style="width: 650px; height: 250px; overflow: auto;"></div>
    </div>
</div>

<script src="../../Areas/HiDoctor_Master/Scripts/HD_Master/MyDocuments.js?v=@HiDoctor.BuildNumber.BuildNo" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {
        AjaxGlobalHandler.Initiate();
        CHUNKUPLOAD.fnGetUploadedFileCount();
        $("#dvAjaxLoad").hide();
    });
</script>
