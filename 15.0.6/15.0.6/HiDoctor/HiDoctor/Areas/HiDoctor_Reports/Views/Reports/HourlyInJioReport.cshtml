@{
    ViewBag.Title = "HourlyInJioReport";
    Layout = null;
}
<link href="../../Content/Bootstrap/bootstrap.css" rel="stylesheet" />

<!-- Design for Reports -->
<div  class="form-container">
<div class="row">
<div  class="col-sm-12 clearfix">
   <div class="col-sm-3 clearfix" id="leftNav">
       <div id="tree" style="float: left; height: auto; width: 100%">
       </div>
   </div>
   <div class="col-sm-9 clearfix">
       <div class="col-sm-12 clearfix">
        <div class="col-sm-6 clearfix"><input type="text" readonly="readonly" class="" id="txtFromDate" /></div>
        <div class="col-sm-6 clearfix"><input type="text" readonly="readonly" class="" id="txtToDate" /></div>
        <div class="col-sm-12 clearfix"><input type="button" id="btnGo" value="Go" class="btn btn-success" /></div>
       </div>
       <div class="col-sm-12 clearfix">
         <div id="dvHourlyReport">

         </div>
       </div>
   </div>
</div>
</div>
</div>


<!-- OverylaY PopUp for Map  -->
<div class="modal" id="dvMoreInfoModal" role="dialog" aria-hidden="true" style="display: none;margin:0px !important;">
    <div class="modal-dialog PopUpMaxSize"  id="PopUpCss">
        <div class="modal-content">
            <div class="modal-header">
                <button aria-label="Close" data-dismiss="modal" class="close" type="button" onclick="HideModalPopup('dvMoreInfoModal');"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title clsModalTitle" id="dvMoreInfoHeader"></h4>
            </div>
            <div class="modal-body">
                <div class="col-lg-12 cls-padding-none" id="dvInfoContent" style="padding: 0px;">
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Scripts are using from  HourlyInJioReport.js-->
<script src="../../Areas/HiDoctor_Reports/Scripts/HD/HourlyInJioReport.js?v=@HiDoctor.BuildNumber.BuildNo"></script>
<script>
    $(document).ready(function () {
        AjaxGlobalHandler.Initiate();
        try {
            HourlyInJio.defaults.cur_User_Code = @Html.Raw(Json.Encode(ViewBag.cur_UserCode));
            HourlyInJio.defaults.Company_Code = @Html.Raw(Json.Encode(ViewBag.Company_Code));
            HourlyInJio.initialize();
            $("#dvAjaxLoad").hide();
        }
        catch (err) {
            $("#dvAjaxLoad").hide();
        }        
       
    });

</script>
    