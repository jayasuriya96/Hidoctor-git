<script type="text/javascript">
    var Month_category="";
    var Year_category="";
    $(document).ready(function () {
        //$('.cls-notification').empty();
        AjaxGlobalHandler.Initiate();
        // $("#notifyhide").hide
        $("#dvAjaxLoad").hide();
        $('#page-header').hide();
        $('#main').css('margin',"0px !important");
      
        AdminDashboard.defaults.Current_Date=@Html.Raw(Json.Encode(ViewBag.Current_Date));
        AdminDashboard.defaults.Previous_Date=@Html.Raw(Json.Encode(ViewBag.Previous_Date));
        AdminDashboard.defaults.Next_Date=@Html.Raw(Json.Encode(ViewBag.Next_Date));
        var cur = @Html.Raw(Json.Encode(ViewBag.Current_Month));
        var pre = @Html.Raw(Json.Encode(ViewBag.Previous_Month));
        var next =@Html.Raw(Json.Encode(ViewBag.Next_Month));
        var IsBack=@Html.Raw(Json.Encode(ViewBag.IsBacks));
        Month_category=@Html.Raw(Json.Encode(ViewBag.Month));
        Year_category=@Html.Raw(Json.Encode(ViewBag.Year));
        AdminDashboard.defaults.Current_Month=cur.split('-')[0];
        AdminDashboard.defaults.Previous_Month=pre.split('-')[0];
        AdminDashboard.defaults.Next_Month=next.split('-')[0];
        AdminDashboard.defaults.Child_User_Count=@Html.Raw(Json.Encode(ViewBag.Child_User_Count))
        AdminDashboard.defaults.Current_Year=cur.split('-')[1];
        AdminDashboard.defaults.Previous_Year=pre.split('-')[1];
        AdminDashboard.defaults.Next_Year=next.split('-')[1];
        AdminDashboard.defaults.Day_After_Tomorrow = @Html.Raw(Json.Encode(ViewBag.Day_After_Tomorrow));
        $("#overylay").overlay().load();
        $('#joineroverylay').overlay().load();
        $('#expandoverylay').overlay().load();
        setTimeout(function () { AdminDashboard.initialize(); }, 50);
        if(IsBack !="YES" || IsBack == null){
            debugger;
            $("#notifyhides").show();
            $("#notifyhide").show();
        }
        else {
            $("#notifyhide").hide();
            $(".dash-title").html('');
            $(".cls-notification ").css('height','0px');
        }

    });
    $("#ExportExceljoiner").click(function(e) {
        debugger;
        var postfix = 'JoinerVsAttrition';
        var a = document.createElement('a');
        var data_type = 'data:application/vnd.ms-excel';
        var table_div = encodeURIComponent($('#dvInfoJoiner').html());
       // var table_html = table_div.outerHTML.replace(/ /g, '%20');
        a.href = data_type + ', ' + table_div;
        a.download = 'JoinerVsAttrition_' + postfix + '.xls';
        a.click();
        e.preventDefault();
    });
    $("#ExportExcel").click(function(e) {
        debugger;
        var postfix = 'OpenPosition';
        var a = document.createElement('a');
        var data_type = 'data:application/vnd.ms-excel';
        var table_div = encodeURIComponent($('#dvInfoJoiner').html());
        // var table_html = table_div.outerHTML.replace(/ /g, '%20');
        a.href = data_type + ', ' + table_div;
        a.download = 'OpenPositionByDivision_' + postfix + '.xls';
        a.click();
        e.preventDefault();
    });
</script>

