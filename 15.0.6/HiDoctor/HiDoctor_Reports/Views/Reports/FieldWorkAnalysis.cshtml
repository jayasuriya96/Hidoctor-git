@{
    ViewBag.Title = "FieldWorkAnalysis";
    Layout = null;
}

<link href="../Areas/HiDoctor_Reports/Content/Reports.css?v=@HiDoctor.BuildNumber.BuildNo" rel="stylesheet" />
<div>
    <div id="dvTree" style="width: 20%; float: left">
        <div id="tree" style="float: left; height: 530px; width: 100%">
        </div>
    </div>
    <div style="width: 80%; float: left" id="divMain">
        <div style="display: none;" id="divToggle">
            <span id="spnTreeToggle" style="text-decoration: underline; cursor: pointer; padding: 5px"
                onclick="fnToggleTree()">Hide Tree</span> | <span id="spnInputToggle" style="text-decoration: underline; cursor: pointer; padding: 5px"
                    onclick="fnToggleInput()">Hide Input</span>
        </div>
        <div id="divInput" style="display: none; padding: 5px">
            <table>
                <tr>
                    <td>Start Date & End Date
                    </td>
                    <td>
                        <input type="text" id="txtFromDate" class='datepicker' />
                        <input type="text" id="txtToDate" class='datepicker' />
                        <input type="hidden" id="hdnmainRegionCode" />
                        <input type="hidden" id="hdnRegionCode" />
                        <input type="hidden" id="hdnUserCode" />
                    </td>
                </tr>
                <tr>
                    <td colspan="2" style="text-align: center">
                        <input type="button" id="btnShow" name="Show" value="Show Report" onclick="fnFieldWorkAnalysis();" />
                    </td>
                </tr>
            </table>
            <div id="dvloading" style="display: none">
                <img src="../../Content/images/loader1.gif" />
            </div>
            <hr />
        </div>

        <div id="divmainReport" style="overflow: auto; width: 100%;">
        </div>

        <div id="divReport" style="padding-top: 20px;"></div>
        
        <div id="divSPECReport" style="padding-top: 20px;"></div>
        
        <div id="divHeaderSummary" style="width: 100%;">
        </div>
        <div id="divReportSummary" style="width: 100%; overflow: auto">
        </div>
        <div id="modal" style="border: 5px solid #676A72; background: #fff; padding: 5px; font-size: 13px; display: none; margin-left: 5%; width: 800px; height: 500px; overflow: auto">
            <span style="float: right; cursor: pointer" onclick="HideModalPopup('modal');">X</span>
            <h2 style="font-size: 15px; border-bottom: 1px solid #efefef; color: #039">Independent Details:</h2>

            <br />
            <div style="clear: both">
            </div>
            <div id="divModel" style="height: 100px;">
            </div>

        </div>
        <div id="modal2" style="border: 5px solid #676A72; background: #fff; padding: 5px; font-size: 13px; display: none; margin-left: 5%; width: 800px; height: 500px; overflow: auto">
            <span style="float: right; cursor: pointer" onclick="HideModalPopup('modal2');">X</span>
            <h2 style="font-size: 15px; border-bottom: 1px solid #efefef; color: #039">Stockist Information:</h2>

            <br />
            <div style="clear: both">
            </div>
            <div id="divModel2" style="height: 100px;">
            </div>

        </div>
    </div>
</div>
<iframe id="ifrmPrint" style="height: 0px; width: 0px;"></iframe>
<div id="divPrint" style="display: none;"></div>


<iframe id="ifrmmainPrint" style="height: 0px; width: 0px;"></iframe>
<div id="divmainPrint" style="display: none;"></div>


<iframe id="ifrmpopPrint" style="height: 0px; width: 0px;"></iframe>
<div id="divpopPrint" style="display: none;"></div>



<iframe id="ifrmRCPAPrint" style="height: 0px; width: 0px;"></iframe>
<div id="divRCPAPrint" style="display: none;"></div>

<iframe id="ifrmSPECPrint" style="height: 0px; width: 0px;"></iframe>
<div id="divSPECPrint" style="display: none;"></div>

<script src="../../Areas/HiDoctor_Reports/Scripts/HD/FieldWorkanalysis.js?v=@HiDoctor.BuildNumber.BuildNo" type="text/javascript"></script>
<script src="../../Areas/HiDoctor_Reports/Scripts/HD/Reports%20level2.js?v=@HiDoctor.BuildNumber.BuildNo" type="text/javascript"></script>
<script src="../../Areas/HiDoctor_Reports/Scripts/HD/MissedCallRecovery.js?v=@HiDoctor.BuildNumber.BuildNo" type="text/javascript"></script>
<script src="../../Areas/HiDoctor_Reports/Scripts/HD/ReportsCommonFiles.js?v=@HiDoctor.BuildNumber.BuildNo" type="text/javascript"></script>
<script type="text/javascript">
    $(function () {
        $(".datepicker").datepicker({
            dateFormat: 'dd/mm/yy',
            numberOfMonths: 2
        });
    });

    $(document).ready(function () {
        AjaxGlobalHandler.Initiate();
        try {

            fnBindRegionTree("tree");
            $("#dvTree").show();
            $('#divInput').hide();
            $('#divToggle').hide();
            $("#dvAjaxLoad").hide();
        }
        catch (err) {
            $("#dvAjaxLoad").hide();
        }

    });

    function fnRegionTreeActivate(node) {
        $("#divReport").html("");
        $("#divmainReport").html("");
        $("#dvTree").hide();
        $('#hdnmainRegionCode').val(node.data.key);
        $('#divInput').show();
        $("#spnTreeToggle").html('Show Tree');
        $('#divToggle').show();
        $("#divMain").css('width', '100%');
    }

    function fnRegionTreePostInit() {

    }

</script>

