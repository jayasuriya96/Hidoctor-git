        type: 'POST',
        data: "",
        url: urls,      
        success: function (data) {
            debugger;
            $("#StartDate").text(startDate);
            $("#EndDate").text(endDate);
            if(data != null){
                DoctorChemistMetTable(data);
                console.log(data);
            }
            else{
                result+="No Data Found";
