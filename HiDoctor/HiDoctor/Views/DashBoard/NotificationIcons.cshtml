@{
    ViewBag.Title = "NotificationIcons";
    Layout = null;
}

<link href="~/Content/Bootstrap/bootstrap.css" rel="stylesheet" />
<link href="~/Content/Bootstrap/awesome-bootstrap-checkbox.css" rel="stylesheet" />
<link href="~/Content/Bootstrap/font-awesome.min.css" rel="stylesheet" />

<style>
    .cls-unapproved-main .panel-body ul {
        display: inline-block;
        list-style: none;
        width: 100%;
    }

        .cls-unapproved-main .panel-body ul li {
            float: left;
            width: 30%;
        }

            .cls-unapproved-main .panel-body ul li p.numbers {
                font-size: 25px;
                margin-bottom: 4px !important;
            }

            .cls-unapproved-main .panel-body ul li p.text {
                font-size: 13px;
                margin-bottom: 0px !important;
            }

    #dvInfoContent {
        max-height: 300px;
        min-height: 200px;
        overflow: auto;
    }

    ul {
        list-style-type: none;
    }

    .notifications {
        display: inline-block;
        list-style: none;
        margin: 11px -10px 0 0;
        padding: 0;
        vertical-align: middle;
        /*float: right;*/
    }

        .notifications > li {
            float: left;
            /*margin: 0 20px 0 0;*/
            margin: 15px 20px 10px -13px;
            position: relative;
        }

            .notifications > li .notification-icon {
                /*background: #FFF;*/
                border-radius: 50%;
                /*box-shadow: 0 2px 2px 0 rgba(0, 0, 0, 0.3);*/
                display: inline-block;
                height: 30px;
                position: relative;
                width: 30px;
                text-align: center;
            }

            .notifications > li > a {
                border: none;
            }

            .notifications > li .notification-icon img {
                color: #404040;
                line-height: 32px;
                vertical-align: middle;
                font-size: 19px;
            }

            .notifications > li .notification-icon .badge {
                background: #FF1744;
                color: #FFF;
                font-size: 12px;
                font-weight: normal;
                height: 19px;
                padding: 3px 5px 3px 5px;
                position: absolute;
                right: -15px;
                top: -11px;
                border-radius: 100%;
                display: inline-block;
                min-width: 19px;
                line-height: 1;
                text-align: center;
                white-space: nowrap;
                vertical-align: middle;
            }
</style>
<div class="">
    <section class="dash-content  form-container" style="padding-left: 30%;" id="divMenu_left">

        <ul class="notifications" id="notifyhides">
            <li>
                @*<div class="cls-dv-separator"></div>*@
            </li>
            <li>
                <a href="#" class="notification-icon messages-pop" data-toggle="dropdown" title="Message">
                    @*   <i class="fa fa-envelope"></i>*@
                    <img src="~/Content/images/Home/ic_message.png" />
                    <span class="badge" id="spnMsg">0</span>
                </a>

            </li>
            </br>
                <li>
                    <a href="#" class="notification-icon notify-pop" data-toggle="dropdown" title="Notification">
                        @* <i class="fa fa-bell"></i>*@<img src="~/Content/images/Home/ic_Alert.png" />
                        <span class="badge" id="spnNotify">0</span>
                    </a>
                </li>
            </br>
                <li>
                    @*<div class="cls-dv-separator"></div>*@
                </li>
            </br>
                <li>
                    <a href="#" class="notification-icon birthday-pop" data-toggle="dropdown" title="Birthday">
                        @*   <i class="fa fa-gift"></i>*@<img src="~/Content/images/Home/ic_birthday.png" />
                        <span class="badge" id="spnBirthday">0</span>
                    </a>
                </li>
            </br>
                <li>
                    <a href="#" class="notification-icon anniversary-pop" data-toggle="dropdown" title="Anniversary">
                        @* <i class="fa fa-calendar-o"></i>*@
                        <img src="~/Content/images/Home/ic_anniversary.png" />
                        <span class="badge" id="spnAnniversary">0</span>
                    </a>
                </li>
            </br>
                 <li>
                     <a href="#" class="notification-icon employee-pop" data-toggle="dropdown" title="Employee Birthday">
                         @* <i class="fa fa-calendar-o"></i>*@
                         <img style="width: 27px;" src="data:image/svg+xml;utf8;base64," />
                         <span class="badge" id="spnEmployee">0</span>
                     </a>
                 </li>
            </br>

        </ul>
    </section>
</div>
<div class="modal" id="dvMoreInfoModal" role="dialog" aria-hidden="true" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button aria-label="Close" data-dismiss="modal" class="close" type="button" onclick="HideModalPopup('dvMoreInfoModal');"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title clsModalTitle" id="dvMoreInfoHeader"></h4>
            </div>
            <div class="modal-body">
                <div class="col-lg-12 cls-padding-none" id="dvInfoContent">
                </div>
            </div>
        </div>
    </div>

</div>

<div class="modal" id="dvMoreInfoModal2" role="dialog" aria-hidden="true" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button aria-label="Close" data-dismiss="modal" class="close" type="button" onclick="HideModalPopup('dvMoreInfoModal2');"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title clsModalTitle" id="dvMoreInfo2Header"></h4>
            </div>
            <div class="modal-body">
                <div class="col-lg-12 cls-padding-none" id="dvInfoContent2">
                </div>
            </div>
        </div>
    </div>

</div>


<div id="dvLoadingDoctorsBirthdayAlerts" style="padding-top: 5px; padding-right: 10px; display: none; width: 700px; min-height: 400px; border: 4px solid #CCCCCC; overflow: scroll; background-color: #FFFFFF;">
    <div style="position: absolute; font-size: 11px; width: 98%; padding: 10px;">
        <span style="float: right; cursor: pointer; color: white; margin: -13px;" onclick="HideModalPopup('dvLoadingDoctorsBirthdayAlerts'); ">@*fnHideAccordinforBday();*@
            <img src="~/Images/close.png" />
        </span>
        <div style="width: auto;" class="cls_AltTitle">Birthday Details</div>
        <br />
        <div id="dvshowBirthdayAlertsforchildusers" style="width: 100%"></div>
    </div>
</div>

<div id="dvLoadingDoctorsAnniversaryAlerts" style="padding-top: 5px; padding-right: 10px; display: none; width: 700px; height: 400px; border: 4px solid #CCCCCC; overflow: scroll; background-color: #FFFFFF;">
    <div style="position: absolute; font-size: 11px; width: 98%; padding: 10px;">
        <span style="float: right; cursor: pointer; color: white; margin: -13px;" onclick="HideModalPopup('dvLoadingDoctorsAnniversaryAlerts');">@*fnHideAccordinforAnniversary();*@
            <img src="~/Images/close.png" />
        </span>
        <div style="width: auto;" class="cls_AltTitle">Anniversary Details</div>
        <br />
        <div id="dvshowAnniversaryAlertsforchildusers" style="width: 100%"></div>
    </div>
</div>

